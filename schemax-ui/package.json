{
  "name": "schemax-ui",
  "version": "1.1.0",
  "description": "SchemaX",
  "author": "wsitm",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build:prod": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "build:stage": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "schema-tool"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/wsitm/SchemaX.git"
  },
  "dependencies": {
    "@sqltools/formatter": "^1.2.5",
    "@univerjs/core": "0.2.11",
    "@univerjs/design": "0.2.11",
    "@univerjs/docs": "0.2.11",
    "@univerjs/docs-ui": "0.2.11",
    "@univerjs/engine-formula": "0.2.11",
    "@univerjs/engine-render": "0.2.11",
    "@univerjs/facade": "0.2.11",
    "@univerjs/find-replace": "0.2.11",
    "@univerjs/network": "0.2.11",
    "@univerjs/rpc": "0.2.11",
    "@univerjs/sheets": "0.2.11",
    "@univerjs/sheets-filter": "0.2.11",
    "@univerjs/sheets-filter-ui": "0.2.11",
    "@univerjs/sheets-find-replace": "0.2.11",
    "@univerjs/sheets-formula": "0.2.11",
    "@univerjs/sheets-numfmt": "0.2.11",
    "@univerjs/sheets-ui": "0.2.11",
    "@univerjs/thread-comment-ui": "0.2.11",
    "@univerjs/ui": "0.2.11",
    "axios": "0.28.1",
    "core-js": "3.37.1",
    "element-ui": "2.15.14",
    "file-saver": "2.0.5",
    "js-cookie": "3.0.1",
    "nprogress": "0.2.0",
    "screenfull": "5.0.2",
    "splitpanes": "2.4.1",
    "vue": "2.6.12",
    "vue-codemirror": "^4.0.6",
    "vue-router": "3.4.9",
    "vuex": "3.6.0",
    "xe-utils": "3.5.18"
  },
  "devDependencies": {
    "@univerjs/webpack-plugin": "0.4.3",
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "4.1.0",
    "compression-webpack-plugin": "6.1.2",
    "connect": "3.6.6",
    "sass": "1.32.13",
    "sass-loader": "10.1.1",
    "script-ext-html-webpack-plugin": "2.1.5",
    "svg-sprite-loader": "5.1.1",
    "vue-template-compiler": "2.6.12"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
